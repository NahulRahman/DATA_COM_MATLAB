clc;
clear;
%N=3;
N=input('');
d=floor(rand(N, N) * 9) + 1;
While(1)
	a=input('');
	if a==1
       for i=1:1:length(d)
            if d(i) == 10
                d(i)=0;
            end
       end
        disp(d);
    elseif a==2
        disp(d');
        
    %elseif a==3;
        
        %print
    else
        disp('Invalid')
      
    end
end